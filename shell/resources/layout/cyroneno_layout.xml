<?xml version="1.0" encoding="utf-8"?>
<RestraintLayout
    id="@+id/rl_cyroneno"
    layout_width="match_parent"
    layout_height="match_parent">

    <ImageView
        id="@+id/iv_cyroneno_img"
        layout_width="fit_content"
        layout_height="fit_content"
        layout_start_handle_id="@id/rl_cyroneno"
        layout_start_handle_margin="8dp"
        layout_top_handle_id="@id/rl_cyroneno"
        layout_top_handle_margin="8dp"
        layout_end_handle_id="@id/rl_cyroneno"
        layout_end_handle_margin="8dp"
        layout_bottom_handle_id="@id/rl_cyroneno"
        layout_bottom_handle_margin="8dp" />

</RestraintLayout>